{
  "name": "tune-chaser",
  "version": "1.0.0",
  "description": "A web page that helps you learn the guitar by comparing microphone input to a played note.",
  "scripts": {
    "format": "prettier --write src",
    "start": "live-server src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx"
  },
  "devDependencies": {
    "@babel/preset-env": "7.26.9",
    "babel-jest": "29.7.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "live-server": "1.2.2",
    "prettier": "3.5.2",
    "eslint": "8.0.1"
  }
}
